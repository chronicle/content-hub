{
    "Creator": "admin",
    "Description": "Example of a simple action in Google SecOps. This action is fetching data from \"api.vatcomply.com\" service based on the provided parameters. This is NOT an Async action. This action DOES NOT work with Google SecOps Entities.",
    "Id": -1,
    "IntegrationIdentifier": "SampleIntegration",
    "IsCustom": false,
    "IsEnabled": true,
    "Name": "SimpleActionExample",
    "Parameters": [
        {
            "DefaultValue": "USD, EUR",
            "Description": "Example of input via CSV values. Comma-separated list of currencies that need to be processed.",
            "IsMandatory": false,
            "Name": "Currencies String",
            "Type": 0,
            "Value": "USD, EUR"
        },
        {
            "DefaultValue": "Select One",
            "Description": "Example of input via DDL. Action will check both \"Currencies String\" and \"Currencies DDL\" to understand what values to process.",
            "IsMandatory": false,
            "Name": "Currencies DDL",
            "OptionalValues": [
                "Select One",
                "USD",
                "EUR",
                "CAD"
            ],
            "Type": 15,
            "Value": "Select One"
        },
        {
            "DefaultValue": "Today",
            "Description": "Time frame for the results. If \"Custom\" is selected, you also need to provide \"Start Time\".",
            "IsMandatory": false,
            "Name": "Time Frame",
            "OptionalValues": [
                "Today",
                "Last 7 Days",
                "Custom"
            ],
            "Type": 15,
            "Value": "Today"
        },
        {
            "DefaultValue": "",
            "Description": "Start time for the results. This parameter is mandatory, if \"Custom\" is selected for the \"Time Frame\" parameter. Format: ISO 8601. Action will only extract the dates from the timestamp. The delta between \"Start Time\" and \"End Time\" can't be greater than 7 days.",
            "IsMandatory": false,
            "Name": "Start Time",
            "Type": 0,
            "Value": ""
        },
        {
            "DefaultValue": "",
            "Description": "End time for the results. Format: ISO 8601. If nothing is provided and \"Custom\" is selected for the \"Time Frame\" parameter then this parameter will use current time. Action will only extract the dates from the timestamp. The delta between \"Start Time\" and \"End Time\" can't be greater than 7 days.",
            "IsMandatory": false,
            "Name": "End Time",
            "Type": 0,
            "Value": ""
        },
        {
            "DefaultValue": true,
            "Description": "Example of input via boolean. If enabled, action will return a JSON Result.",
            "IsMandatory": true,
            "Name": "Return JSON Result",
            "Type": 1,
            "Value": true
        }
    ],
    "ScriptResultName": "is_success",
    "Version": 1.0
}